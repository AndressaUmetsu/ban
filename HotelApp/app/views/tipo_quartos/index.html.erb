<p id="notice"><%= notice %></p>

<h1>Tipo Quartos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Preco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_quartos.each do |tipo_quarto| %>
      <tr>
        <td><%= tipo_quarto.nome %></td>
        <td><%= tipo_quarto.preco %></td>
        <td><%= link_to 'Show', tipo_quarto %></td>
        <td><%= link_to 'Edit', edit_tipo_quarto_path(tipo_quarto) %></td>
        <td><%= link_to 'Destroy', tipo_quarto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Quarto', new_tipo_quarto_path %>
