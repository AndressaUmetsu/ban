<p id="notice"><%= notice %></p>

<h1>Tipo Servicos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Custo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_servicos.each do |tipo_servico| %>
      <tr>
        <td><%= tipo_servico.nome %></td>
        <td><%= tipo_servico.custo %></td>
        <td><%= link_to 'Show', tipo_servico %></td>
        <td><%= link_to 'Edit', edit_tipo_servico_path(tipo_servico) %></td>
        <td><%= link_to 'Destroy', tipo_servico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Servico', new_tipo_servico_path %>
